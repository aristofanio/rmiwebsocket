<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">
  <display-name>RMI WebSocket Sample App</display-name>
  
  <servlet>
    <servlet-name>TempConversion</servlet-name>
    <servlet-class>com.lambdazen.websocket.sample.TempConversionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TempConversion</servlet-name>
    <url-pattern>/TempConversion</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>CallTest</servlet-name>
    <servlet-class>com.lambdazen.websocket.sample.CallTestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CallTest</servlet-name>
    <url-pattern>/CallTest</url-pattern>
  </servlet-mapping>  
</web-app>
